{
	"name": "@discord-user-card/vue",
	"type": "module",
	"version": "0.0.8",
	"description": "Display Discord users' profile cards just like in Discord.",
	"keywords": [
		"discord",
		"card",
		"profile",
		"discord-user-card",
		"vue",
		"vue3"
	],
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"import": "./lib/index.js",
			"require": "./lib/index.cjs"
		},
		"./style.css": "./lib/style.css"
	},
	"main": "./lib/index.cjs",
	"module": "./lib/index.js",
	"unpkg": "./lib/index.umd.cjs",
	"types": "./lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build && vue-tsc --emitDeclarationOnly",
		"types": "vue-tsc",
		"preview": "vite preview"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"dependencies": {
		"discord-user-card": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^22.0.3",
		"@vitejs/plugin-vue": "^5.1.2",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"vue": "^3.4.35",
		"vue-tsc": "^2.0.29"
	}
}
