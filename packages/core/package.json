{
	"name": "@discord-user-card/core",
	"version": "1.0.0",
	"description": "The core behind the Discord User Card project.",
	"type": "module",
	"main": "./lib/index.js",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.js",
			"types": "./lib/index.d.ts"
		},
		"./style.css": "./lib/style.css"
	},
	"scripts": {
		"build:ts": "tsc",
		"build:scss": "sass src/styles/index.scss lib/style.css",
		"dev:ts": "tsc -w",
		"dev:scss": "sass src/styles/index.scss lib/style.css --watch",
		"dev": "pnpm dev:ts & pnpm dev:scss",
		"build": "pnpm build:ts && pnpm build:scss"
	},
	"files": [
		"lib"
	],
	"keywords": [
		"discord",
		"card",
		"profile",
		"discord-user-card"
	],
	"devDependencies": {
		"discord-api-types": "^0.37.84",
		"sass": "^1.77.2"
	}
}
